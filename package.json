{
  "name": "email-assistant",
  "version": "0.3.0",
  "private": true,
  "main": "./public/main.js",
  "homepage": "./",
  "files": [
    "build",
    "*.js",
    "public"
  ],
  "dependencies": {
    "@apollo/client": "^3.9.6",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@chakra-ui/react": "^1.8.9",
    "@electron/remote": "^2.1.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@tanstack/react-table": "^8.13.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "date-and-time": "^2.0.1",
    "dayjs-business-days": "^1.0.4",
    "electron-dl": "^3.5.2",
    "electron-is-dev": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "framer-motion": "^4.1.17",
    "graphql": "^16.8.1",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-notifications-component": "^3.4.1",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^5.0.1",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^8.3.2",
    "wait-on": "^7.2.0",
    "web-vitals": "^1.0.1",
    "webpack": "^5.90.3",
    "zustand": "^4.5.2"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently -k \"cross-env BROWSER=none npm start\" \"npm run electron:start\"",
    "electron:start": "wait-on tcp:3000 && electron-forge start",
    "package": "electron-forge package",
    "make": "npm run build && electron-forge make",
    "make-mac": "electron-forge make --platform darwin",
    "electron:build": "npm run build && electron-builder -c.extraMetadata.main=build/main.js"
  },
  "build": {
    "productName": "Ashland Assistant",
    "extends": null,
    "appId": "com.ashland.ashlandassistant",
    "files": [
      "dist/**/*",
      "build/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "icon": "build/logoicns.icns"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.1",
    "@electron-forge/cli": "^6.4.2",
    "@electron-forge/maker-deb": "^6.4.2",
    "@electron-forge/maker-dmg": "^7.3.0",
    "@electron-forge/maker-rpm": "^6.4.2",
    "@electron-forge/maker-squirrel": "^6.4.2",
    "@electron-forge/maker-zip": "^6.4.2",
    "@electron-forge/plugin-auto-unpack-natives": "^6.4.2",
    "autoprefixer": "^10.4.18",
    "electron": "^29.1.0",
    "postcss": "^8.4.35",
    "react-app-rewired": "^2.2.1",
    "tailwindcss": "^3.4.1"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./src/assets/icons/logo.icns",
        "name": "Ashland Assistant"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "Ashland Assistant",
            "icon": "./src/assets/icons/logo.icns"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "name": "Ashland Assistant"
          }
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {
            "name": "Ashland Assistant"
          }
        },
        {
          "name": "@electron-forge/maker-dmg",
          "config": {
            "format": "ULFO",
            "name": "Ashland Assistant",
            "icon": "./src/assets/icons/logo.icns"
          }
        }
      ]
    }
  }
}
