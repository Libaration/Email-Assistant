{
  "name": "Ashland Assistant",
  "version": "0.3.0",
  "private": true,
  "main": "./public/main.js",
  "homepage": "./",
  "files": [
    "build",
    "*.js",
    "public"
  ],
  "dependencies": {
    "@apollo/client": "^3.9.7",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@electron/remote": "^2.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@tanstack/react-table": "^8.13.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "date-and-time": "^3.1.1",
    "dayjs-business-days": "^1.0.4",
    "electron-deeplink": "github:glawson/electron-deeplink",
    "electron-dl": "^3.5.2",
    "electron-is-dev": "^3.0.1",
    "electron-squirrel-startup": "^1.0.0",
    "framer-motion": "^11.0.12",
    "graphql": "^16.8.1",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "node-gyp": "^10.0.1",
    "node-sass": "^9.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-icons": "^5.0.1",
    "react-router-dom": "^5.3.4",
    "react-scripts": "^5.0.1",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "update-electron-app": "^3.0.0",
    "uuid": "^9.0.1",
    "wait-on": "^7.2.0",
    "web-vitals": "^3.5.2",
    "webpack": "^5.90.3",
    "zustand": "^4.5.2"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently -k \"cross-env BROWSER=none npm start\" \"npm run electron:start\"",
    "electron:start": "wait-on tcp:3000 && electron-forge start",
    "package": "electron-forge package",
    "make": "npm run build && electron-forge make",
    "make-mac": "electron-forge make --platform darwin",
    "electron:build": "npm run build && electron-builder -c.extraMetadata.main=build/main.js",
    "publish": "electron-forge publish --platform=darwin"
  },
  "build": {
    "productName": "Ashland Assistant",
    "extends": null,
    "appId": "com.ashland.ashlandassistant",
    "files": [
      "dist/**/*",
      "build/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "icon": "build/logoicns.icns"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "forge": "./forge.config.js"
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.1",
    "@electron-forge/cli": "^7.3.0",
    "@electron-forge/maker-deb": "^7.3.0",
    "@electron-forge/maker-dmg": "^7.3.0",
    "@electron-forge/maker-rpm": "^7.3.0",
    "@electron-forge/maker-squirrel": "^7.3.0",
    "@electron-forge/maker-zip": "^7.3.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.3.0",
    "autoprefixer": "^10.4.18",
    "electron": "^29.1.3",
    "postcss": "^8.4.35",
    "react-app-rewired": "^2.2.1",
    "tailwindcss": "^3.4.1"
  }
}
